{
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "name": "server",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src && tsc",
    "devstart": "npm run build && npm run start",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register --transpile-only src/app.ts",
    "tsc-build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "npm run lint && rimraf dist && npm run tsc-build",
    "start": "node dist/app.js",
    "format": "prettier --write ."
  },
